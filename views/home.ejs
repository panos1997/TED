<% include partials/header.ejs %>

<% if(currentUser !== undefined && currentUser.role === "seller" && currentUser.request !== "pending") {  %>
	<link rel="stylesheet" type="text/css" href="/seller.css">
<% }  %>
<% if(currentUser !== undefined && currentUser.role === "bidder" && currentUser.request !== "pending") {  %>
	<link rel="stylesheet" type="text/css" href="/bidder.css">
<% }  %>
	

  <!-- Masthead -->
  <header class="masthead  text-center">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-md-10 col-lg-8 col-xl-7 mx-auto" id="searchDiv">
          <form action = "/search" method = "POST">
            <div class="form-row">
     	      	   <div class="col-4 col-md-4 " >					  
						 <select class="mdb-select md-form" name="auction[category]" >
						 	 <option value="" disabled selected>Choose Category</option>
							  <option value="home" >Home & Garden</option>
							  <option value="Fashion">Fashion & Jewlery</option>
							  <option value="Health">Health</option>
							  <option value="Sports">Sports</option>
							  <option value="Books">Books</option>
							  <option value="Movies">Games & Consoles</option>
							  <option value="Toys">Toys</option>
							  <option value="Electronics">Electronics</option>
							  <option value="Art">Art</option>
							  <option value="Houses">Properties</option>
							  <option value="Vehicles">Vehicles</option>
						  </select>
					</div>
						 
	              <div class="col-6 col-md-6 ">
	                <input class="form-control form-control-lg" name = "auction[name]" placeholder="Search for anything here...">
	              </div>

	              <div class="col-2 col-md-2">
	                <button type="submit" id="search" class="btn btn-block btn-lg btn-primary">Search</button>
	              </div>          
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </header>


<%  if(currentUser !== undefined)  {        %>

	<%	if(currentUser.role === "seller" && currentUser.request !== "pending") {   %>
			<div class="container" style="width: 100%; display: table; margin-top: -4em;">
				<div class="sellerPicture">
					<a href="/auctions">
						<h3 class="sellerPictureTitle" style="margin-left; color: #000033; "><strong> My auctions </strong></h3>
						<img  src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT0izdhYBGg--nU2VvKHWNwPCRPzxyG_TITnJEMEEWwDzn3VGrFhQ" style="width:100%; border: 1px solid black" class="zoom img-thumbnail rounded-circle float-left" >
					</a>
				</div>
				<div class="sellerPicture">
					<a  href="/auctions/new" >
						<h3 class="sellerPictureTitle" style="margin-left: color: #000033; "> <strong> Start new auction </strong> </h3>
						<img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSbmvQY1v2saBrkHCq--T9sTZXM5Qj94nQIU_dR-LUctaPuZpPGHg" style="width:100%; border:1px solid black" class="zoom img-thumbnail rounded-circle float-right" >
					</a>
				</div>	
			</div>
	<%	} %>
		
		
		
	<%	if(currentUser.role === "bidder" && currentUser.request !== "pending") {   %>
			<div class="container" style="width: 100%; display: table; margin-top: -4em;">
				<div class="sellerPicture">
					<a href="/categories">
						<h3 class="sellerPictureTitle"><strong> Auctions Search </strong></h3>
						<img src="data:image/jpeg;base64," style="width:100%; border: 1px solid black" class="zoom img-thumbnail rounded-circle float-left" >						
					</a>
				</div>
				<div class="sellerPicture">
					<a  href="/bids">
						<h3 class="sellerPictureTitle "><strong>My Bids </strong></h3>
						<img src="data:image/jpeg;base64," style="width:100%; border: 1px solid black" class="zoom img-thumbnail rounded-circle float-left" >						
					</a>						
				</div>
			</div>			
	<%	} %>
		
	<%	if(currentUser.request === "pending") {   %>
	
			<!--Carousel Wrapper-->
			<div id="carousel-thumb" class="carousel slide carousel-fade carousel-thumbnails" data-interval="3000" data-ride="carousel">
				<!--Slides-->
				<div class="carousel-inner" role="listbox">
					<div class="carousel-item active">
						<img class="d-block w-100" src="Summer-Graphic 1.png"
						alt="First slide">
					</div>
					<div class="carousel-item">
						<img class="d-block w-100" src="kisspng-service-guarantee-customer-satisfaction-money-back-chuck-norris-5acc59b143e546.8592248915233417452781.jpg"
						alt="Second slide">
					</div>
					<div class="carousel-item">
						<img class="d-block w-100"  src="Delivery.jpg"
						alt="Third slide">
					</div>
				</div>
				<!--/.Slides-->
				<!--Controls-->
				<a class="carousel-control-prev" href="#carousel-thumb" role="button" data-slide="prev">
					<span class="carousel-control-prev-icon" aria-hidden="true"></span>
					<span class="sr-only">Previous</span>
				</a>
				<a class="carousel-control-next" href="#carousel-thumb" role="button" data-slide="next">
					<span class="carousel-control-next-icon" aria-hidden="true"></span>
					<span class="sr-only">Next</span>
				</a>
				<!--/.Controls-->
				<ol class="carousel-indicators">
					<li data-target="#carousel-thumb" data-slide-to="0" class="active">
						<img src="https://mdbootstrap.com/img/Photos/Others/Carousel-thumbs/img%20(88).jpg" width="100">
					</li>
					<li data-target="#carousel-thumb" data-slide-to="1">
						<img src="https://mdbootstrap.com/img/Photos/Others/Carousel-thumbs/img%20(121).jpg" width="100">
					</li>
					<li data-target="#carousel-thumb" data-slide-to="2">
						<img src="https://mdbootstrap.com/img/Photos/Others/Carousel-thumbs/img%20(31).jpg" width="100">
					</li>
				</ol>
			</div>
			<!--/.Carousel Wrapper-->
	
	
	
			<header class=" jumbotron jumbotron-fluid" id="jumbHome">
				<div class="container">
					
					<h1><b><font color="black">Welcome to <font color="red">e</font><font color="blue">b</font><font color="yellow">a</font><font color="green">y</font> Team.</font></b></h1>
					<p><font color="black">Ebay team is the best site for markets in the world.<br>
