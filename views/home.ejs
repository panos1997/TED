<% include partials/header.ejs %>


  <!-- Masthead -->
  <header class="masthead  text-center">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-md-10 col-lg-8 col-xl-7 mx-auto" id="searchDiv">
          <form action = "/search" method = "POST">
            <div class="form-row">
     	            <div class="col-4 col-md-4 ">					
							  <a class="btn btn-primary dropdown-toggle " id="categoriesButton" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
							    choose category
							  </a>
							  <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
							  	<input type="radio" name="auction[category]" value = "Home" > Home <br>
							  	<input type="radio" name="auction[category]" value = "Fashion" > Fashion <br>
							  	<input type="radio" name="auction[category]" value = "Health" > Health <br>
							  	<input type="radio" name="auction[category]" value = "Sports" > Sports <br>
							  	<input type="radio" name="auction[category]" value = "Books" > Books <br>
							  	<input type="radio" name="auction[category]" value = "Movies" > Movies <br>
							  	<input type="radio" name="auction[category]" value = "Toys" > Toys <br>
							  	<input type="radio" name="auction[category]" value = "Electronics" > Electronics <br>
							  	<input type="radio" name="auction[category]" value = "Art" > Art <br>
							  	<input type="radio" name="auction[category]" value = "Houses" > Houses <br>
							  	<input type="radio" name="auction[category]" value = "Vehicles" > Vehicles <br>
							  
							  </div>
						
	               </div>  
	              <div class="col-6 col-md-6 ">
	                <input class="form-control form-control-lg" name = "auction[name]" placeholder="Search for anything here...">
	              </div>

	              <div class="col-2 col-md-2">
	                <button type="submit" id="search" class="btn btn-block btn-lg btn-primary">Search</button>
	              </div>          
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </header>
  



<%  if(currentUser !== undefined)  {        %>

	<%	if(currentUser.role === "seller" && currentUser.request !== "pending") {   %>
			<div class="container">
				<div>

					<a href="/auctions" class="sellerPicture">
						<h3 class="sellerPictureTitle">My auctions</h3>
						<img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT0izdhYBGg--nU2VvKHWNwPCRPzxyG_TITnJEMEEWwDzn3VGrFhQ" style="width:100%; border: 1px solid black" class="zoom img-thumbnail rounded-circle float-left" >
					</a>
					<a  href="/auctions/new" class="sellerPicture">
						<h3 class="sellerPictureTitle"> Start new auction</h3>
						<img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSbmvQY1v2saBrkHCq--T9sTZXM5Qj94nQIU_dR-LUctaPuZpPGHg" style="width:100%; border:1px solid black" class="zoom img-thumbnail rounded-circle float-right" >
					</a>
					
				</div>
			</div>
	<%	} %>
		
		
		
	<%	if(currentUser.role === "bidder" && currentUser.request !== "pending") {   %>
			<div class="container">
				<div>
					<a class="sellerPicture" href="/categories">
						<h3 class="sellerPictureTitle">Search for an auction</h3>
						<img src="data:image/jpeg;base64," style="width:100%; border: 1px solid black" class="zoom img-thumbnail rounded-circle float-left" >						
					</a>
					<a class="sellerPicture" href="/bids">
						<h3 class="sellerPictureTitle ">My bids</h3>
						<img src="data:image/jpeg;base64," style="width:100%; border: 1px solid black" class="zoom img-thumbnail rounded-circle float-left" >						
					</a>						
				</div>
			</div>			
	<%	} %>
		
	<%	if(currentUser.request === "pending") {   %>
		<div class="container">
			<header class="jumbotron jumbotron-fluid" id="jumbHome">
				<div class="container">
					
					<h1><b><font color="black">Welcome to <font color="red">e</font><font color="blue">b</font><font color="yellow">a</font><font color="green">y</font> Team.</font></b></h1>
					<p><font color="black">Ebay team is the best site for markets in the world.<br>
					Î¤he site was created by the best developers to help you make quick and easy markets.</font></p>
						
						
					<div class="form-group">
						<a class="btn btn-secondary btn-sm" href="/categories">Continue as a guest</a>
					</div>
						
				<div>
			</div>
		</div>
	<% } %>
		
		
		
		
		
		
<%  } else { %>
	<div class="container">
		<header class="jumbotron jumbotron-fluid" id="jumbHome">
			<div class="container">
			
				<h1><b><font color="black">Our <font color="red">E</font><font color="blue">b</font><font color="yellow">i</font><font color="green">d</font> Team is here to fullfill your needs.</font></b></h1>
				<p><font color="black">Here you can sell your products or buy products from all over the world.<br>
				What you need is just a few clicks away.</font></p>
				
				
				<div class="form-group">
					<a class="btn btn-secondary btn-sm" href="/categories">Continue as a guest</a>
				</div>
				
			<div>
		</div>
	</div>



  <!-- Icons Grid -->
  <section class="features-icons text-center">
    <div class="container">
      <div class="row">
        <div class="col-lg-6">
          <div class="features-icons-item mx-auto mb-5 mb-lg-0 mb-lg-3">
            <div class="features-icons-icon d-flex">
              <i class="icon-screen-desktop m-auto text-primary"></i>
            </div>
            <h3>I am a <strong> seller </strong></h3>
            <a href="/">
           	 	<img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTUVkpNhl-xWZZ0Qe_gX_-rh3w16Cw4NdBT2HItaNFw9Iak_BZlGQ">
            </a> 
            <p class="lead mb-0">A seller can create new auctions and accept the top bids for each of his products</p>
          </div>
        </div>
        <div class="col-lg-6">
          <div class="features-icons-item mx-auto mb-5 mb-lg-0 mb-lg-3">
            <div class="features-icons-icon d-flex">
              <i class="icon-layers m-auto text-primary"></i>
            </div>
            <h3> I am a<strong> bidder </strong> </h3>
            <a href="/">
            	<img src="data:image/jpeg;base64,">
            </a>
            <p class="lead mb-0">A bidder can naviate himself to the active auctions and decide which of them he wants to claim</p>
          </div>
        </div>

      </div>
    </div>
  </section>

<% } %>


